<% include ../partials/_header %>




<div class="site-blocks-cover inner-page-cover overlay" style="background-image: url(https://res.cloudinary.com/dsk4fft6r/image/upload/v1656179374/hero_bg_4_oulsx4.jpg);" data-aos="fade" data-stellar-background-ratio="0.5">
    <div class="container">
      <div class="row align-items-center justify-content-center text-center">
        <div class="col-md-10">
           <h1 class="mb-2">Detalles</h1>
        </div>
      </div>
    </div>
  </div>
  
  <div class="site-section site-section-sm">
    <div class="container">
      <div class="row">
        <div class="col-lg-8">
          <div>
            <div class="slide-one-item home-slider owl-carousel">
              <div><img src="<%= proddos.imageuno %>" alt="Image" class="img-fluid"></div>
              <div><img src="<%= proddos.imagedos %>" alt="Image" class="img-fluid"></div>
              <div><img src="<%= proddos.imagetres %>" alt="Image" class="img-fluid"></div>
            </div>
          </div>
          <div class="bg-white property-body border-bottom border-left border-right">
            <div class="row mb-5">
              <div class="col-md-6">
                <strong class="text-success h1 mb-3">€<%= proddos.price %></strong>
              </div>
              <div class="col-md-6">
                <ul class="property-specs-wrap mb-3 mb-lg-0  float-lg-right">
                <li>
                  <span class="property-specs">
                    <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_20/v1656340529/descarga_1_smf4c5.png">
                  </span>
                  <span class="property-specs-number"><%= proddos.colorstock %> <sup> </sup></span>
                  
                </li>
                <li>
                   <span class="property-specs">
                      <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_20/v1656178991/square-metre-outline-information-icon-260nw-2011557692_bl7q40.webp" alt="">
                  </span>
                  <span class="property-specs-number"><%= proddos.talle %></span>
                  
                </li>
                <li>
                    <span class="property-specs">
                       <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_25/v1656346048/1606239_n4aolu.png" alt="">
                   </span>
                   <span class="property-specs-number"><%= proddos.color %></span>
                   
                 </li>
  
  
                 <li>
                    <span class="property-specs">
                       <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_20/v1656340397/182657_ycmmol.png" alt="">
                   </span>
                   <span class="property-specs-number"><%= proddos.talle %></span>
                   
                 </li>
  
  
                 <li>
                    <span class="property-specs">
                       <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_25/v1656346234/istockphoto-1280237461-1024x1024_vye6fq.jpg" alt="">
                   </span>
                   <span class="property-specs-number"><%= proddos.amount %></span>
                   
                 </li>
  
                 <li>
                    <span class="property-specs">
                       <img src="https://res.cloudinary.com/dsk4fft6r/image/upload/c_scale,w_25/v1656346496/6b544957aa2b12011d899c4ca4f2d198_fbxgl5.png" alt="">
                   </span>
                   <span class="property-specs-number"><%= proddos.dolarprice %></span>
                   
                 </li>
                 
              </ul>
              </div>
            </div>
